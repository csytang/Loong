<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.jdt.core.IJavaProject"
            id="LoongPluginFMRTool.contribution">
         <menu
               id="LoongPlugin.FMRTool.FMRecoveryJ"
               label="LoongFMR"
               path="additions">
            <separator
                  name="group1">
            </separator>
            <separator
                  name="group2">
            </separator>
         </menu>
         <action
               class="loongpluginfmrtool.popup.actions.OpenFeatureNameRecommendationView"
               enablesFor="1"
               icon="icons/ConfigurationIcon.png"
               id="LoongPlugin.FMRTool.architecturerecoveryjava"
               label="Open Feature Configuration Editor"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryJ/group1">
         </action>
         <action
               class="loongpluginfmrtool.popup.actions.OpenFeatureNameRecommendationView"
               icon="icons/recommend.png"
               id="LoongPluginFMRTool.recommendName"
               label="Open Feature Name Recommendation View"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryJ/group1">
         </action>
         <action
               class="loongpluginfmrtool.popup.actions.RecoveryFeatureModel"
               icon="icons/recovery.png"
               id="LoongPluginFMRTool.action3"
               label="Recovery Feature Model"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryJ/group1">
         </action>
         <action
               class="loongpluginfmrtool.popup.actions.BuildModules"
               icon="icons/macro.ico"
               id="LoongPluginFMRTool.modulebuilder"
               label="Build #ifdef modules"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryJ/group1">
         </action>
         <action
               class="loongpluginfmrtool.popup.actions.CreateMDGFile"
               icon="icons/mpgtoolbox.png"
               id="LoongPluginFMRTool.bunchtool"
               label="Generate MDG file (Bunch Toolbox)"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryJ/group2">
         </action>
         <action
               class="loongpluginfmrtool.popup.actions.CreateRSFFile"
               id="LoongPluginFMRTool.action4"
               label="Generate RSF file (ACDC Toolbox)"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryJ/group2">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="LoongPluginFMRTool.objectContributioncpp"
            objectClass="org.eclipse.cdt.core.model.ICProject">
         <menu
               id="LoongPlugin.FMRTool.FMRecoveryC"
               label="LoongFMR"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="loongpluginsartool.popup.actions.CPPProjectSARecovery"
               icon="icons/recovery.png"
               id="LoongPluginSARTool.architecturerecoverycpp"
               label="Open Feature Configuration Editor"
               menubarPath="LoongPlugin.FMRTool.FMRecoveryC/group1">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="loongpluginfmrtool.editor.configfeaturemodeleditor.ui.ConfigurableFeatureModelEditor"
            contributorClass="loongpluginfmrtool.editor.configfeaturemodeleditor.actions.DiagramActionBarContributor"
            default="true"
            extensions="mconfig"
            icon="icons/mconfig.gif"
            id="LoongPlugin.FMRTool.mConfigFeatureModelEditor"
            name="Configurable Feature Model Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="LoongPlugin.viewcategory"
            class="loongpluginfmrtool.views.recommendedfeatureview.RecommendedFeatureView"
            icon="icons/recommend.png"
            id="LoongPlugin.FMRTool.recommendedFeatureList"
            name="Recommended Feature List"
            restorable="true">
      </view>
      <category
            id="LoongPlugin.viewcategory"
            name="Loong">
      </category>
      <view
            category="LoongPlugin.viewcategory"
            class="loongpluginfmrtool.views.moduleviews.ModuleViewPart"
            icon="icons/module.png"
            id="LoongPlugin.FMRTool.ModuleView"
            name="Modules with Configurations"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="LoongPlugin.newwizard"
            class="loongpluginfmrtool.newWizard.MConfigFileWizard"
            icon="icons/ConfigurationFileSmall.png"
            id="LoongPluginSARTool.mConfigFeatureModelwizard"
            name="Temprorary Configurable Feature Model">
      </wizard>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
   </extension>

</plugin>
